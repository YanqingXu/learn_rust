{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "ü¶Ä Debug Main Program",
            "cargo": {
                "args": [
                    "build",
                    "--bin=main",
                    "--package=learn_rust"
                ],
                "filter": {
                    "name": "main",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal",
            "environment": [
                {
                    "name": "RUST_LOG",
                    "value": "debug"
                },
                {
                    "name": "RUST_BACKTRACE",
                    "value": "1"
                }
            ],
            "preLaunchTask": "rust: cargo build main",
            "sourceLanguages": ["rust"]
        },
